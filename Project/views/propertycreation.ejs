  <div class="container">
    <p style="text-align: center; font-size: 20px; font-weight: 700;">Airbnb your home</p>
    <form method="post" action="/api/property/create" name="Create property" enctype="multipart/form-data">
      <div class="mb-3">
        <label for="description" class="form-label">Property Name</label>
        <input name="description" type="text" class="form-control" id="description" placeholder="Enter your property name" aria-describedby="namelHelp">
      </div>
      <div class="mb-3">
        <label for="image" class="form-label">Upload Property Image</label>
        <input name="image" type="file">
      </div>
      <div class="mb-3">
        <label for="location" class="form-label">Location of property</label>
        <input name="location" type="text" placeholder="Eg. Bhubaneswar" class="form-control" id="location" aria-describedby="emailHelp">
      </div>
      <div class="mb-3">
        <label for="price" class="form-label">Price in INR/night</label>
        <input name="price" type="number" placeholder="Enter Price" class="form-control" id="price" aria-describedby="mobileHelp">
      </div>
      <!-- <div class="mb-3">
        <label for="password" class="form-label">Password</label>
        <input name= "password" type="password" class="form-control" id="password">
      </div> -->
      <!-- <div class="mb-3 form-check">
        <input type="checkbox" class="form-check-input" id="exampleCheck1">
        <label class="form-check-label" for="exampleCheck1">Check me out</label>
      </div> -->
      <button type="submit" class="btn btn-primary">Host your property</button>
      <a class="btn btn-primary" href="/" role="button">Go Back</a>
    </form>
  </div>
  <script src="script.js"></script>
